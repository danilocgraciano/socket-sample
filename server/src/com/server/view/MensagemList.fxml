        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="101.0" text="Cliente" fx:id="colCliente" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="150.0" text="Data Hora" fx:id="colDataHora" />
